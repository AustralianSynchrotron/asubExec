record (seq, "MIDPT:ASUB:EXEC:INIT") {
    field (DESC, "")
    field (SCAN, "Passive")
    field (PINI, "YES")
    field (SELM, "All")
    field (DLY1, "0.5")
    field (DOL1, "1")
    field (LNK1, "MIDPT:ASUB:EXEC.DISV")
}

record (aSub, "MIDPT:ASUB:EXEC") {
    field (DESC, "Invokes mid_points.py")
    field (SCAN, "Passive")

    info (EXEC, "mid_points.py")

    # Set to default DISA - so that the record disabled at start
    # up to inhibit initial CP processing
    #
    field (DISV, "0")

    field (INAM, "asubExecInit")
    field (SNAM, "asubExecProcess")

    field (INPA, "MIDPT:INPA CP")
    field (FTA,  "DOUBLE")
    field (NOA,  "8")

    field (OUTA, "MIDPT:OUTA PP")
    field (FTVA, "DOUBLE")
    field (NOVA, "7")

    field (OUTB, "MIDPT:OUTA PP")
    field (FTVB, "LONG")
    field (NOVB, "5")

    field (OUTC, "MIDPT:OUTA PP")
    field (FTVC, "CHAR")
    field (NOVC, "5")

    field (OUTD, "MIDPT:OUTA PP")
    field (FTVD, "STRING")
    field (NOVD, "5")
}

record (waveform, "MIDPT:INPA") {
    field (DESC, "mid_points input")
    field (SCAN, "Passive")
    field (NELM, "8")
    field (FTVL, "DOUBLE")
}

record (waveform, "MIDPT:OUTA") {
    field (DESC, "mid_points output")
    field (SCAN, "Passive")
    field (NELM, "7")
    field (FTVL, "DOUBLE")
}

record (waveform, "MIDPT:OUTB") {
    field (DESC, "Integer out")
    field (SCAN, "Passive")
    field (NELM, "5")
    field (FTVL, "LONG")
}

record (waveform, "MIDPT:OUTC") {
    field (DESC, "Char out")
    field (SCAN, "Passive")
    field (NELM, "5")
    field (FTVL, "CHAR")
}

record (waveform, "MIDPT:OUTD") {
    field (DESC, "String out")
    field (SCAN, "Passive")
    field (NELM, "5")
    field (FTVL, "STRING")
}

# end
